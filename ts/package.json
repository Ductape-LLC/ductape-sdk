{
  "name": "ductape-sdk",
  "version": "1.0.14",
  "description": "sdk for using ductaped integrations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "serve": "nodemon ./src/index.ts",
    "build": "tsc",
    "test": "npm test",
    "test:appBuilder": "nodemon src/test.appBuilder.ts",
    "test:integrationBuilder": "nodemon src/test.integrationBuilder.ts",
    "test:processor": "nodemon src/test.processor.ts",
    "test:utils": "nodemon src/test.utils.ts",
    "test:imports": "nodemon src/test.imports.ts",
    "docs": "typedoc --out docs src"
  },
  "author": "Sanni Ifedayo",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "crypto-js": "^4.2.0",
    "joi": "^17.7.0",
    "typescript": "^4.9.4",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@types/xml2js": "^0.4.11",
    "@types/xml2json": "^0.11.4",
    "nodemon": "^3.0.1"
  }
}
